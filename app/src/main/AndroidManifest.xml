<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ciscoproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Government Schemes"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ListOfSchemesActivity"
            android:theme="@style/AppTheme.NoActionBar"/>


        <activity android:name=".activity.ComplaintActivity" />
        <activity android:name=".activity.LoginAcitivity" />
        <activity
            android:name=".activity.SchemesActivity"
            android:label="@string/title_activity_schemes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.ResetPasswordActivity" />
        <activity android:name=".activity.SignUpActivity" />
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>